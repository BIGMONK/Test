<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".activity.SDCardPathActivity">

    <TextView
        android:id="@+id/tv_title_inner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="内置卡" />


    <TextView
        android:id="@+id/tv_inner_sdcard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_title_inner" />

    <TextView
        android:id="@+id/tv_title_extral"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_inner_sdcard"
        android:text="外置卡" />

    <TextView
        android:id="@+id/tv_extral_sdcard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_title_extral" />

    <TextView
        android:id="@+id/tv_title_usb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_extral_sdcard"
        android:text="USB存储" />

    <TextView
        android:id="@+id/tv_usb_store"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_title_usb" />

</RelativeLayout>
