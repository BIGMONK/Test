<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_circle_image_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.uto.djf.test.view.RoundCornerImageView
        android:layout_width="200px"
        android:layout_height="200px"
        android:scaleType="centerCrop"
        android:src="@drawable/bg_map_line"
        custom:Round="200" />

    <com.uto.djf.test.view.MiniMapView
        android:id="@+id/minimapview"
        android:layout_width="100px"
        android:layout_height="100px"
        android:background="#DD0000"
        custom:view_height="990px"
        custom:view_width="990px" />
</LinearLayout>
